<% @stations.each do |station| %>
<h3>Station</h3>
  <ul class="station">
    <li class="name">Name: <%= station.name %></li>
    <li class="address">Address: <%= station.address %></li>
    <li class="fuel_types">Fuel Type: <%= station.fuel_types %></li>
    <li class="distance">Distance: <%= station.distance %> mi</li>
    <li class="access_times">Access Times: <%= station.access_times %></li>
  </ul>
<% end %>
